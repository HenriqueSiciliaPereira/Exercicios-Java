import java.util.Scanner;

public class imprimir_impares {

	public static void main(String[] args) {
		
		Scanner sc = new Scanner(System.in);
	    
		System.out.println("Até qual número deseja ver os impares?");
		int x = sc.nextInt();
		
		
		for (int i = 0; i <= x; i++) {
			
			if ( i % 2 == 1 ) {
				
				System.out.println(i);
				
			}
			
		}
		
		
		
		
		
		sc.close();

	}

}
